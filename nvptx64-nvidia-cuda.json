{
  "abi-blacklist": [
    "cdecl",
    "stdcall",
    "fastcall",
    "vectorcall",
    "thiscall",
    "aapcs",
    "win64",
    "sysv64",
    "msp430-interrupt",
    "x86-interrupt",
    "amdgpu-kernel"
  ],
  "arch": "nvptx64",
  "cpu": "sm_50",
  "features": "+ptx60",
  "data-layout": "e-i64:64-i128:128-v16:16-v32:32-n16:32:64",
  "dll-prefix": "",
  "dll-suffix": ".ptx",
  "dynamic-linking": true,
  "env": "",
  "exe-suffix": ".ptx",
  "executables": true,
  "linker": "rust-lld",
  "linker-flavor": "ld.lld",
  "llvm-target": "nvptx64-nvidia-cuda",
  "max-atomic-width": 64,
  "merge-functions": "disabled",
  "obj-is-bitcode": true,
  "os": "cuda",
  "requires-lto": true,
  "relocation-model": "static",

  "panic-strategy": "abort",
  "target-c-int-width": "32",
  "target-endian": "little",
  "target-pointer-width": "64",
  "vendor": "nvidia"
}
